    "    import numpy as np\n",
    "    x = input()\n",
    "    y = input()\n",
    "    xx = np.array(str.split(x))\n",
    "    yy = np.array(str.split(y))\n",
    "    n = min(len(xx), len(yy))\n",
    "    out_idx = np.flatnonzero(xx[:n] == yy[:n])\n",
    "    out_val = xx[out_idx]\n",
    "    print(len(np.unique(out_val)))"
