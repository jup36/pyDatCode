   "source": [
    "def pig_latin(text): \n",
    "    words = text.split()\n",
    "    pigged_text = []\n",
    "    for word in words:\n",
    "        word = word[1:]+word[0]+'ay'\n",
    "        pigged_text.append(word)\n",
    "    return ' '.join(pigged_text)\n",
    "    \n",
    "print(pig_latin(\"hello how are you\"))"
   ]
